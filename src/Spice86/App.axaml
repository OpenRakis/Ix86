    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Spice86">
    <Application.Styles>
        <FluentTheme Mode="Dark" />
        <!-- Add the line below to get OxyPlot UI theme applied. -->
